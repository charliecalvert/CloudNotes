{
    "name": "cloudnotes",
    "version": "1.0.0",
    "description": "CloudNotes ==========",
    "main": "remote.js",
    "type": "module",
    "scripts": {
        "start": "DEBUG=check-main,check-main-detail node src/CheckMarkdown.js",
        "test": "jest",
        "test-check-both": "DEBUG=check-markdown-both jest --watch CheckMarkdown-both",
        "test-check-toc": "DEBUG=check-markdown-toc jest --watch CheckMarkdown-toc",
        "create-new-ids": "DEBUG=check-main,check-main-detail node src/RecreateAllIds.js",
        "git add-js": "scripts/addjs",
        "chkmd": "/usr/bin/node --inspect --inspect-brk node --runInBand src/CheckMarkdown.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/charliecalvert/CloudNotes.git"
    },
    "keywords": [
        "remote"
    ],
    "author": "Charlie Calvert",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/charliecalvert/CloudNotes/issues"
    },
    "homepage": "https://github.com/charliecalvert/CloudNotes#readme",
    "dependencies": {
        "debug": "^4.3.4",
        "elven-code": "^4.12.1",
        "gray-matter": "^4.0.3",
        "markdown-elf": "^1.0.0",
        "markdown-toc": "^1.2.0",
        "ssh2": "^1.10.0",
        "walk-directories": "^4.0.0"
    },
    "devDependencies": {
        "eslint": "^8.16.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "jest": "^28.1.0",
        "markdownlint-cli": "^0.31.1"
    }
}