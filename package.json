{
    "name": "cloudnotes",
    "version": "1.0.0",
    "description": "CloudNotes ==========",
    "type": "module",
    "main": "remote.js",
    "scripts": {
        "start": "DEBUG=check-md nodemon CheckMarkdown.js",
        "test": "NODE_OPTIONS=--experimental-vm-modules DEBUG=check-get-elf jest",
        "basic": "node lib/walk-markdown-files.mjs",
        "lint": "eslint . --ext .js,.mjs",
        "lint-fix": "eslint . --ext .js,.mjs --fix",
        "msl": "DEBUG=tools:modify-symlink-source nodemon Tools/get-sym-links/modify-symlink-source.mjs",
        "msd": "DEBUG=tools:modify-symlink-source:result nodemon Tools/get-sym-links/modify-symlink-source.mjs",
        "msf": "DEBUG=tools:modify-symlink-source:find nodemon Tools/get-sym-links/modify-symlink-source.mjs",
        "msld": "DEBUG=DEBUG=tools:modify-symlink-source,tools:modify-symlink-source:result nodemon Tools/get-sym-links/modify-symlink-source.mjs",
        "cff": "DEBUG=tools:create-forward-files node Tools/get-sym-links/create-foward-files.js",
        "cffd": "DEBUG=tools:create-forward-files:content node Tools/get-sym-links/create-foward-files.js",
        "InsertFrontMatterNotWorking": "DEBUG=lib:walk-markdown-files:summary node Tools/insert-front-matter.mjs",
        "InsertFrontMatterNotWorking2": "DEBUG=lib:walk-markdown-files:summary,lib:walk-markdown-files:fileInfos node Tools/insert-front-matter.mjs"
        },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/charliecalvert/CloudNotes.git"
    },
    "keywords": [
        "remote"
    ],
    "author": "Charlie Calvert",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/charliecalvert/CloudNotes/issues"
    },
    "homepage": "https://github.com/charliecalvert/CloudNotes#readme",
    "dependencies": {
        "chalk": "^5.2.0",
        "debug": "^4.3.4",
        "elven-code": "^4.14.2",
        "elven-jekyll-post": "^1.2.5",
        "gray-matter": "^4.0.3",
        "luxon": "^3.3.0",
        "markdown-elf": "^1.0.0",
        "markdown-toc": "^1.2.0",
        "ssh2": "^1.14.0",
        "supports-color": "^9.3.1",
        "url": "^0.11.1",
        "walk-directories": "^4.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.3.1",
        "eslint": "^8.43.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^16.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22"
    }
}
